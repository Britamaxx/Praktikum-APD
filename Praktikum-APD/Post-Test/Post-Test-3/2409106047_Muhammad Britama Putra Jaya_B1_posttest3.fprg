        <attribute name="about" value=""/>
        <attribute name="saved" value="2024-09-23 10:17:44 PM"/>
        <attribute name="created" value="bXkgYXN1cztCUklUQU1BOzIwMjQtMDktMjI7MTI6Mjc6MjAgUE07MjQ5Nw=="/>
        <attribute name="edited" value="bXkgYXN1cztCUklUQU1BOzIwMjQtMDktMjM7MTA6MTc6NDQgUE07NDsyNjEy"/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="menu" type="Integer" array="False" size=""/>
            <declare name="panjangsisi, lebarsisi, keliling, luas, jarijari" type="Real" array="False" size=""/>
            <output expression="&quot;Menu Menghitung Keliling/Luas Bangun Datar : &quot;&amp;&#13;&#10;ToChar(13)&amp;&quot;(Pilih Salah Satu)&quot;&amp;&#13;&#10;ToChar(13)&amp;&quot;1. Keliling Persegi&quot;&amp;&#13;&#10;ToChar(13)&amp;&quot;2. Luas Persegi&quot;&amp;&#13;&#10;ToChar(13)&amp;&quot;3. Keliling Persegi Panjang&quot;&amp;&#13;&#10;ToChar(13)&amp;&quot;4. Luas Persegi Panjang&quot;&amp;&#13;&#10;ToChar(13)&amp;&quot;5. Keliling Lingkaran&quot;&amp;&#13;&#10;ToChar(13)&amp;&quot;6. Luas Lingkaran&quot;" newline="True"/>
            <input variable="menu"/>
            <if expression="menu = 1">
                <then>
                    <output expression="&quot;Masukkan Panjang Sisi : &quot;" newline="True"/>
                    <input variable="panjangsisi"/>
                    <assign variable="keliling" expression="panjangsisi*4"/>
                    <output expression="&quot;Keliling Persegi adalah &quot;&amp;keliling&amp;&quot; cm&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="menu = 2">
                        <then>
                            <output expression="&quot;Masukkan Panjang Sisi : &quot;" newline="True"/>
                            <input variable="panjangsisi"/>
                            <assign variable="luas" expression="panjangsisi^2"/>
                            <output expression="&quot;Luas Persegi adalah &quot;&amp;luas&amp;&quot; cm^2&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="menu = 3">
                                <then>
                                    <output expression="&quot;Masukkan Panjang : &quot;" newline="True"/>
                                    <input variable="panjangsisi"/>
                                    <output expression="&quot;Masukkan Lebar : &quot;" newline="True"/>
                                    <input variable="lebarsisi"/>
                                    <assign variable="keliling" expression="panjangsisi*2+lebarsisi*2"/>
                                    <output expression="&quot;Keliling Persegi Panjang adalah &quot;&amp;keliling&amp;&quot; cm&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="menu = 4">
                                        <then>
                                            <output expression="&quot;Masukkan Panjang : &quot;" newline="True"/>
                                            <input variable="panjangsisi"/>
                                            <output expression="&quot;Masukkan Lebar : &quot;" newline="True"/>
                                            <input variable="lebarsisi"/>
                                            <assign variable="luas" expression="panjangsisi*lebarsisi"/>
                                            <output expression="&quot;Luas Persegi Panjang adalah &quot;&amp;luas&amp;&quot; cm^2&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="menu = 5">
                                                <then>
                                                    <output expression="&quot;Masukkan Jari-Jari : &quot;" newline="True"/>
                                                    <input variable="jarijari"/>
                                                    <assign variable="keliling" expression="2*22/7*jarijari"/>
                                                    <output expression="&quot;Keliling Lingkaran adalah &quot;&amp;keliling&amp;&quot; cm&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <if expression="menu = 6">
                                                        <then>
                                                            <output expression="&quot;Masukkan Jari-Jari : &quot;" newline="True"/>
                                                            <input variable="jarijari"/>
                                                            <assign variable="luas" expression="22/7*jarijari^2"/>
                                                            <output expression="&quot;Luas Lingkaran adalah &quot;&amp;luas&amp;&quot; cm^2&quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <output expression="&quot;Menu Tidak Tersedia :)&quot;" newline="True"/>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
